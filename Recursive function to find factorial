#include<stdio.h>
#include<conio.h>
int main()
{
int n,X=0;
printf("Enter the number=>");
scanf("%d",&n);
X=fact(n);
      printf("The factorial is=>%d",X);

getch();
}
int fact(int n)
{
    if(n==1)

        return 1;

    else if(n==0)

          return 1;

    else

            return n*fact(n-1);


    return 0;
}
