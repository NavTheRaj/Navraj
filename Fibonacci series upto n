#include<stdio.h>
#include<conio.h>
void fibonacci()
{
    int a,first=0,second=1,next,i;
    printf("Enter the number limit\n");
    scanf("%d",&a);
    for(i=1;i<=a;i++)
    {
        printf("%d\t",first);
        next=first+second;
        first=second;
        second=next;
          }

}
int main()
{
   fibonacci();
   getch();
   return 0;
}
