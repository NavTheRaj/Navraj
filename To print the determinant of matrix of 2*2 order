#include<stdio.h>
#include<conio.h>
int main()
{
int a[2][2],i,j,det;
printf("Enter the elements of the matrix\n");
for(i=0;i<2;i++)
{
    for(j=0;j<2;j++)
    {
        scanf("%d",&a[i][j]);
    }
}
det=a[0][0]*a[1][1]-a[1][0]*a[0][1];
printf("The determination of the matrix is:%d",det);
getch();
return 0;
}

