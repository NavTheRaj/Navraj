/*Author :Navraj Khanal
  Program:Program to print to check if a number is divisible by 5 & 11 by using ternary operator.
  Date   :1/3/2018  */
  #include<stdio.h>
  #include<conio.h>
  int main()
  {
   int a;
   printf("Enter the number\n");
   scanf("%d",&a);
   (a%5==0 && a%11==0)?printf("Divisible by both"):printf("Not divisible by both");
   getch();
  }

