#include<stdio.h>
#include<conio.h>
void prime()
{
    int n,i,count=0;
    printf("Enter the number\n");
    scanf("%d",&n);
    for(i=2;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==1)
    {
        printf("It is prime");
    }
    else
        {
    printf("It is not a prime number.");
    }
}
int main()
{
    prime();
    return 0;
    getch();
}
